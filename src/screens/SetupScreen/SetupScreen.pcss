.setup-screen {
  display: flex;
  height: 100vh;
  justify-content: flex-start;
  width: 100%;

  &__left {
    display: flex;
    width: 50%;
  }

  &__right {
    display: flex;
    width: 50%;

    &-wrapper {
      background: #fff;
      display: flex;
      flex-direction: column;
      padding: 72px;
      width: 100%;
    }
  }
}
