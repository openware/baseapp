  --header-market-selector-text-color: var(--primary-text-color);
  --header-toolbar-item-text-color: var(--secondary-contrast-cta-color);
  --header-switcher-border-color-theme: var(--secondary-contrast-cta-color);
  --header-switcher-background-color-theme-active: var(--dropdown-background-color);
  --header-positive-text-color: var(--bids);
  --header-negative-text-color: var(--asks);
