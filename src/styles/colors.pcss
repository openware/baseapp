  --body-background-color-level-4: rgba(var(--rgb-body-background-color), var(--alpha-level-4));
  --body-background-color-level-5: rgba(var(--rgb-body-background-color), var(--alpha-level-5));
  --body-background-color-level-6: rgba(var(--rgb-body-background-color), var(--alpha-level-6));
  --body-background-color-level-7: rgba(var(--rgb-body-background-color), var(--alpha-level-7));
  --body-background-color-level-8: rgba(var(--rgb-body-background-color), var(--alpha-level-8));

  /* =================================
      3. Subheader background color opacity area
    ================================ */
