  /*  ================================
      11. System red color opacity area
    ================================ */
  --system-red-level-1: rgba(var(--rgb-system-red), var(--alpha-level-1));
  --system-red-level-2: rgba(var(--rgb-system-red), var(--alpha-level-2));
  --system-red-level-3: rgba(var(--rgb-system-red), var(--alpha-level-3));
  --system-red-level-4: rgba(var(--rgb-system-red), var(--alpha-level-4));
  --system-red-level-5: rgba(var(--rgb-system-red), var(--alpha-level-5));
  --system-red-level-6: rgba(var(--rgb-system-red), var(--alpha-level-6));
  --system-red-level-7: rgba(var(--rgb-system-red), var(--alpha-level-7));
  --system-red-level-8: rgba(var(--rgb-system-red), var(--alpha-level-8));

  /*  ================================
      12. System yellow color opacity area
    ================================ */
  --system-yellow-level-1: rgba(var(--rgb-system-yellow), var(--alpha-level-1));
  --system-yellow-level-2: rgba(var(--rgb-system-yellow), var(--alpha-level-2));
