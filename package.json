{
  "name": "@openware/base-app",
  "version": "2.3.25",
  "private": true,
  "dependencies": {
    "@openware/components": "2.0.46",
    "@openware/cryptofont": "^0.1.6",
    "axios": "^0.18.0",
    "bitcoincashjs": "0.1.10",
    "bootstrap": "^4.3.1",
    "history": "^4.7.2",
    "i18n-iso-countries": "^3.7.8",
    "immutability-helper": "^3.0.0",
    "js-yaml": "^3.12.0",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "react": "^16.4.2",
    "react-date-picker": "^7.0.0",
    "react-dom": "^16.4.2",
    "react-fade-in": "^0.1.6",
    "react-ga": "^2.6.0",
    "react-google-recaptcha": "^1.0.4",
    "react-grid-layout": "^0.16.6",
    "react-intl": "^2.8.0",
    "react-maskinput": "^1.0.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "^2.17.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.2",
    "sinon": "^7.2.3",
    "tslint-eslint-rules": "^5.4.0",
    "ua-parser-js": "^0.7.19",
    "webpack": "^3.11.0"
  },
  "scripts": {
    "build:css": "postcss src/index.pcss -o src/index.css",
    "watch:css": "postcss src/index.pcss -o src/index.css -w",
    "lint": "tsc --noEmit && tslint --project .",
    "lint:fix": "tslint --project . --fix",
    "mockAPI": "node mocks/mockAPI.js",
    "mockWS": "node mocks/mockWS.js",
    "start": "npm-run-all -p watch:css start-js mockAPI mockWS",
    "start-js": "react-app-rewired start --scripts-version react-scripts-ts start",
    "build-js": "react-app-rewired build --scripts-version react-scripts-ts build",
    "build": "npm-run-all build:css build-js clean:mapfiles",
    "clean:mapfiles": "find build -name '*.map' -exec rm {} ';'",
    "test": "jest",
    "test:coverage": "jest --collectCoverage=true",
    "test:ci": "jest --ci",
    "spec:integration": "cypress open",
    "eject": "react-scripts-ts eject",
    "clean": "rimraf build",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "devDependencies": {
    "@openware/coding-standards": "0.1.6",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/history": "^4.7.0",
    "@types/jest": "^23.3.1",
    "@types/jquery": "^3.3.6",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^6.0.6",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-test-renderer": "^16.0.2",
    "@types/webpack-env": "^1.13.6",
    "autoprefixer": "^9.1.1",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "cssnano": "^4.0.5",
    "cypress": "^3.1.5",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "file-loader": "^1.1.11",
    "husky": "^1.0.0-rc.13",
    "isomorphic-ws": "^4.0.1",
    "jest": "^23.6.0",
    "mockserver": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-autoreset": "^2.0.2",
    "postcss-cli": "^6.0.0",
    "postcss-hexrgba": "^1.0.1",
    "postcss-import": "^12.0.0",
    "postcss-nested": "^3.0.0",
    "ramda": "^0.26.1",
    "react-app-rewired": "^1.6.2",
    "react-hot-loader": "^4.3.4",
    "react-test-renderer": "^16.4.2",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.2",
    "source-map-explorer": "^1.7.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1",
    "webpack-obfuscator": "^0.18.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "./scripts/validate_commit.js $HUSKY_GIT_PARAMS && (cd \"./$GIT_PREFIX\"; yarn lint)",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "printWidth": 130,
    "semi": true,
    "singleQuote": true,
    "tslintIntegration": true,
    "eslintIntegration": true,
    "stylelintIntegration": true,
    "ignorePath": ".gitignore"
  }
}
